<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.Pane?>

<Pane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.group4_icms.Functions.VC.Controller.PolicyOwnerCustomerController" prefWidth="1600.0" prefHeight="1024.0">
    <VBox prefHeight="1024.0" prefWidth="1600.0" spacing="10" stylesheets="@../css/home.css">
        <TextField fx:id="filterField" promptText="Enter search term..." prefWidth="1000.0" stylesheets="@../css/table.css"/>
        <TableView fx:id="tableView" prefWidth="1600" prefHeight="550" stylesheets="@../css/table.css">
            <columns>
                <TableColumn text="Customer ID" fx:id="colCId" prefWidth="100"/>
                <TableColumn text="Full Name" fx:id="colFullName" prefWidth="150"/>
                <TableColumn text="Phone Number" fx:id="colPhoneNumber" prefWidth="150"/>
                <TableColumn text="Email" fx:id="colEmail" prefWidth="150"/>
                <TableColumn text="Address" fx:id="colAddress" prefWidth="150"/>
                <TableColumn text="Role" fx:id="colRole" prefWidth="100"/>
                <TableColumn text="Insurance Card" fx:id="colInsuranceCard" prefWidth="150"/>
                <TableColumn text="Policy Owner ID" fx:id="colPolicyOwner_Id" prefWidth="150"/>
                <TableColumn text="Effective Date" fx:id="colEffectiveDate" prefWidth="100"/>
                <TableColumn text="Expiration Date" fx:id="colExpirationDate" prefWidth="100"/>
                <TableColumn text="Delete" fx:id="colDelete" prefWidth="80"/>
            </columns>
        </TableView>
        <StackPane fx:id="contentArea" VBox.vgrow="ALWAYS">
            <!-- Additional content can be added here -->
        </StackPane>
    </VBox>
</Pane>
